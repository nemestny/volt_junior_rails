<!DOCTYPE html>
<html>
  <head>
    <title>VoltJuniorRails</title>

    <%= stylesheet_link_tag    'application', media: 'all' %>
  </head>

  <body>
    <div class="container">
      <header>
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
          <%= link_to "VoltJuniorRails", root_path, class: 'navbar-brand' %>
          <% if @current_user %>
            <%= link_to "Logout", logout_path, class: "btn btn-primary my-2 my-sm-0 text-right", role: "button" %>
          <% end %>
        </nav>
      </header>
      <main>
        <%= yield %>
      </main>
      <footer>
        
      </footer>
    </div>
  </body>
</html>
